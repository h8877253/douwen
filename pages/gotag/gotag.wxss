page{background: #F3F3F3;}
checkbox .wx-checkbox-input{
border-radius: 20rpx;
height: 40rpx;
width: 40rpx;
background: rgba(0, 0, 0, .4);
border: 0;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
height: 58rpx;
width: 58rpx;
line-height: 58rpx;
background: #05DD4C;
background-size: 100%;
color: #FFF;
text-align: center;
