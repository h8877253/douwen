.Grid {
  text-align: left;
  padding:0 40rpx;
}
.Grid-cell {
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  text-align: center;
  margin: 20rpx 35rpx 0 0;
  border-radius: 20rpx;
  position: relative;
}
.Grid-cell image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
  }
  .Grid-cell text{
  position: absolute;bottom:0;left:0;width: 200rpx;height: 50rpx;line-height:50rpx;font-size: 24rpx;background: rgba(0, 0, 0, .4);color: #FFF;border-bottom-right-radius:10px;   border-bottom-left-radius:10px;  }


    .Grid-cell checkbox{
      position: absolute;
      width:200rpx;
      height:200rpx;
      line-height: 25px;
      left:0;
      top: 0;
      text-align: right;
    }
      .Grid  .Grid-cell:nth-child(3n){
          margin-right: 0;
    }
.title-label{color: #333;font-size: 42rpx;text-align: left;width: 100%;height: 80rpx;line-height: 80rpx;text-indent: 20rpx;font-weight: 500;}
.title-text{color: #B0B0B0;font-size: 32rpx;text-align: left;width: 100%;height: 40rpx;text-indent: 20rpx;}
.biaoqiansub{position: fixed;bottom: 0;background: #FFF;color: #4585C5;width: 100%;border-radius: 0;text-align: center;height: 100rpx;line-height: 100rpx;border: 0;}
